{
 "name": "cloud-router",
 "version": "0.0.6",
 "description": "Router for web worker based backends such as CloudFlare Workers.",
 "author": "Cogneco AB",
 "license": "MIT",
 "repository": "https://github.com/cogneco/cloud-router",
 "bugs": {
  "url": "https://github.com/cogneco/cloud-router/issues"
 },
 "homepage": "https://github.com/cogneco/cloud-router#readme",
 "private": false,
 "main": "dist/index.js",
 "typings": "dist/index.d.ts",
 "git": {
  "tagName": "v${version}"
 },
 "github": {
  "release": true
 },
 "scripts": {
  "dev": "watch 'npm run build' .",
  "lint": "eslint '**/*.{js,ts,tsx}'",
  "fix": "eslint '**/*.{js,ts,tsx}' --fix",
  "build": "tsc -p .",
	"test:clean": "rimraf ./transpiled/*",
	"test": "npm run test:clean && npm run transpile && mocha --require source-map-support/register --recursive transpiled",
	"transpile": "tsc --project tsconfig.test.json",
	"prepare": "npm run build",
  "clean": "rm -rf dist node_modules coverage"
 },
 "dependencies": {
  "cloud-http": "0.0.7"
 },
 "devDependencies": {
  "@types/chai": "^4.2.14",
  "@types/chai-subset": "^1.3.3",
  "@types/mocha": "^8.0.4",
  "@typescript-eslint/eslint-plugin": "4.8.2",
  "@typescript-eslint/parser": "4.8.2",
  "chai": "^4.2.0",
  "chai-subset": "^1.6.0",
  "eslint": "7.14.0",
  "eslint-plugin-prettierx": "github:nilssonemma/eslint-plugin-prettierx#master",
  "mocha": "^8.2.1",
  "prettierx": "github:simonmika/prettierx#newline-before-clause",
  "rimraf": "^3.0.2",
  "ts-loader": "^8.0.11",
  "typescript": "^4.1.2",
  "webpac": "0.0.1-security",
  "webpack-cli": "^4.2.0"
 }
}
